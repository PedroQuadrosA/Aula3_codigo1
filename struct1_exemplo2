/*

EXEMPLO 2

*/

#include <stdio.h>

struct veiculo{
  int  ano;
  int  potencia;
};

int main(){

  int i;
  
  struct veiculo vei[3];
  
  for( i = 0; i < 3; i++){
  	
    printf("Ano: ");
    scanf("%d", &vei[i].ano);
    printf("Potencia: ");
    scanf("%d", &vei[i].potencia);
    
  }
  
  for(i = 0; i < 3; i++){
  	
		printf("\nAno: %d | potencia: %d\n", vei[i].ano ,vei[i].potencia);
   
  }
  
}
